	### fierce
	echo -e "${GREEN}    [ $(date +'%Y-%m-%d %H:%M:%S') ] Download fierce ${END}"
	apt -y  install fierce --upgrade > /dev/null 2>&1
	ea 'fierce'
	sleep 2

	### subfinder
	echo -e "${GREEN}    [ $(date +'%Y-%m-%d %H:%M:%S') ] Download subfinder${END}"
	go install -v github.com/projectdiscovery/subfinder/v2/cmd/subfinder@latest > /dev/null 2>&1
	ea 'subfinder'
	sleep 2

	### knock
	echo -e "${GREEN}    [ $(date +'%Y-%m-%d %H:%M:%S') ] Download knock ${END}"
