	echo -e "${GREEN}    [ $(date +'%Y-%m-%d %H:%M:%S') ] Download altdns ${END}"
	apt -y  install --upgrade altdns > /dev/null 2>&1
	ea 'altdns'
	sleep 2

	### Brute force
}

